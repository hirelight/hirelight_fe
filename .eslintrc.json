{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/button-has-type": "error",
    "react/react-in-jsx-scope": "off",
    "import/order": [
      2,
      {
        "groups": [
          "builtin",
          "external",
          "internal",
          "parent",
          "sibling",
          "index"
        ],
        "newlines-between": "always"
      }
    ],
    "import/newline-after-import": 1
  }
}
